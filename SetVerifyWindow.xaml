<Window x:Class="CounterMoney.SetVerifyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CounterMoney"
        mc:Ignorable="d"
        Title="Установка минут" Height="100" Width="200">
    <StackPanel>
        <TextBlock Text="Всего минут:" />
        <TextBox x:Name="MinutesTextBox" />
        <Button Content="OK" Click="OnSetClick" />
    </StackPanel>
</Window>
